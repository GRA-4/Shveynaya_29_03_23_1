<Window x:Class="Shveynaya_29_03_23_1.AuthWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Shveynaya_29_03_23_1"
        mc:Ignorable="d"
        Title="AuthWindow" Height="310" Width="400">
    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal" Height="50" VerticalAlignment="Top" HorizontalAlignment="Center" >
                <Button x:Name="GoAuthButton" Click="GoAuthButton_Click" Content="Авторизация" Width="120" Margin="5"/>
                <Button x:Name="GoRegButton" Click="GoRegButton_Click" Content="Регистрация" Width="120" Margin="5"/>
            </StackPanel>
            <Frame x:Name="Start" NavigationUIVisibility="Hidden"  />
        </StackPanel>
    </Grid>
</Window>
